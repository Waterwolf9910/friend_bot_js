{
    "compilerOptions": {
        "module": "CommonJS",
        "strict": false,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        // "lib": [],
        "paths": {
            "ws_proto": ["./ws_proto.d.ts"],
            "main/*": ["./src/*"]

        },
        "jsx": "react-jsx",
        "moduleResolution": "Node",
        "rootDir": "src",
        "target": "ESNext",
        "sourceMap": true,
        "outDir": "build",
        "removeComments": true,
        "resolveJsonModule": true,
        "declaration": false,
        "extendedDiagnostics": true,
        "esModuleInterop": true,
        "diagnostics": true
    },
    "watchOptions": {"watchDirectory": "dynamicprioritypolling"},
    "typeAcquisition": {"enable": true},
    "buildOptions": {"verbose": true},
    "include": ["./src"],
    "ts-node": {"preferTsExts": true, "compilerOptions": {"jsx": "react-jsx", "allowArbitraryExtensions": true}},
}
