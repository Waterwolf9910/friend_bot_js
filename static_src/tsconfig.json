{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "module": "CommonJS",
        "strict": true,
        "importHelpers": false,
        "forceConsistentCasingInFileNames": true,
        "lib": ["DOM"],
        "outDir": "",
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": false,
        "jsx": "react-jsx",
        "target": "ES2022",
        "rootDir": ".",
        "sourceMap": true,
    },
    "include": [
        "**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
