{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "module": "CommonJS",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "lib": ["DOM"],
        "outDir": "",
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": false,
        "jsx": "react-jsx",
        "target": "ES2020",
        "rootDir": ".",
    },
    "include": [
        "**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
